#define dbg(...) __dbg(#__VA_ARGS__, __VA_ARGS__)
template <typename Arg1>
void __dbg(const char* name, Arg1&& arg1){cerr << name << " : " << arg1 << std::endl;}
template <typename Arg1, typename... Args>
void __dbg(const char* names, Arg1&& arg1, Args&&... args){const char* comma = strchr(names + 1, ',');cerr.write(names, comma - names) << " : " << arg1<<" | ";__dbg(comma+1, args...);}


#define _N 300005

struct coder {
  int x, y, index;
};

bool operator<(coder a, coder b) {
  if (a.x == b.x) {
    return a.y < b.y;
  }
  return a.x < b.x;
}

int N;
coder arr[_N];
int bit[_N];

void update(int x) {
  for (; x <= _N; x += x & -x) bit[x]++;
}

int query(int x) {
  int sum = 0;
  for (; x > 0; x -= x & -x) sum += bit[x];
  return sum;
}

int ans[_N];

int main() {
  // sublimeProblem;
  fast_io;

  cin >> N;
  FOR(i, 1, N + 1) {
    cin >> arr[i].x >> arr[i].y;
    arr[i].index = i;
  }

  sort(arr + 1, arr + N + 1);

  for (int i = 1; i <= N;) {
    int e = i;
    while (e <= N && arr[i].x == arr[e].x && arr[i].y == arr[e].y) e++;

    int xx = query(arr[i].y);
    for (int j = i; j < e; j++) ans[arr[j].index] = xx;
    for (int j = i; j < e; j++) update(arr[j].y);

    i = e;
  }

  FOR(i, 1, N + 1)
  cout << ans[i] << endl;
}
