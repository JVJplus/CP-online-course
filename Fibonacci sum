#define dbg(args...) { __print(__split(#args, ',').begin(), args); cerr << endl; }
vector<string> __split(const string& s, char c) {
    vector<string> v;
    stringstream ss(s);
    string x;
    while (getline(ss, x, c))
        v.emplace_back(x);
    return v;
}
void __print(vector<string>::iterator) {}
template<typename T, typename... Args>
void __print(vector<string>::iterator it, T a, Args... args) {
    string name = it -> substr((*it)[0] == ' ', it -> length());
    if (isalpha(name[0]))
        cerr << name  << " = " << a << " ";
    else
        cerr << name << " ";
    __print(++it, args...);
}


#define fast_io                        ios_base::sync_with_stdio(false);cin.tie(NULL);
#define sublimeProblem                 freopen("input.txt", "r", stdin); freopen("output.txt", "w", stdout); freopen("debug_log.txt", "w", stderr);


const int N = 100005;

void matrixMul(ll a[2][2],ll b[2][2]){
    ll mul[2][2];
    For(i,2){
        For(j,2){
            mul[i][j]=0;
            For(k,2){
                mul[i][j]=(mul[i][j]+(a[i][k]*b[k][j])%MOD)%MOD;
            }
        }
    }

    //storing back the answer in a[][]
    For(i,2){
        For(j,2){
            a[i][j]=mul[i][j];
        }
    }
}


void matrixPowRecursive(ll a[2][2],int n){
    if(n<=1)
        return;
    matrixPowRecursive(a,n/2);
    matrixMul(a,a);
    if(n&1){
        ll base[2][2]={{1,1},{1,0}};
        matrixMul(a,base);
    }
}

ull matrixPow(ll a[2][2],ll b){
    ll base[2][2]={{1,1},{1,0}};
    ll ans[2][2]={{1,0},{0,1}};
    while(b){
        if(b&1){
            matrixMul(ans,a);
        }
        matrixMul(a,a);
        b>>=1;
    }
    return ans[0][0]%MOD;
}

ll nthFib(int n){
    if(n==0)
        return 0;
    ll base[2][2]={{1,1},{1,0}};
    return matrixPow(base,n-1);
}

unsigned long long fiboSum(unsigned long long a,unsigned long long b)
{
    return (nthFib(b+2)-nthFib(a+1)+MOD)%MOD;
}

int main()
{
    fast_io;
    sublimeProblem;

    TCase{
        int a,b;
        cin>>a>>b;
        cout << fiboSum(a,b) << endl;
    }
          
}
