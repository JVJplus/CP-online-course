#define dbg(...) __dbg(#__VA_ARGS__, __VA_ARGS__)
template <typename Arg1>
void __dbg(const char* name, Arg1&& arg1){cerr << name << " : " << arg1 << std::endl;}
template <typename Arg1, typename... Args>
void __dbg(const char* names, Arg1&& arg1, Args&&... args){const char* comma = strchr(names + 1, ',');cerr.write(names, comma - names) << " : " << arg1<<" | ";__dbg(comma+1, args...);}


#define _N 100005
int v,e;
vector<pii> g[_N];

ll dijsktra(int u1,int v1){
	if(u1==v1)
		return INF;

	vector<int> dist(v+1,INF);
	vector<bool> vis(v+1,0);

	dist[u1] = 0;
    multiset < pair < int , int > > s;          // multiset do the job as a min-priority queue

    s.insert({0 , u1});                          // insert the source node with distance = 0

    while(!s.empty()){

        pair <int , int> p = *s.begin();        // pop the vertex with the minimum distance
        s.erase(s.begin());

        int x = p.se; int wei = p.fi;
        if( vis[x] ) continue;                  // check if the popped vertex is visited before
        vis[x] = true;

        for(int i = 0; i < g[x].size(); i++){
        	int e1 = g[x][i].fi; int w = g[x][i].se;
            if(dist[x] + w < dist[e1]  ){            // check if the next vertex distance could be minimized
            	dist[e1] = dist[x] + w;
                s.insert({dist[e1],  e1} );           // insert the next vertex with the updated distance
            }
        }
    }
    return dist[v1];
}
